{
  "name": "run-subdir",
  "version": "1.0.0",
  "description": "",
  "preferGlobal": true,
  "bin": {
    "rsd": "bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MohannadNaj/run-subdir.git"
  },
  "keywords": [
    "run-subdir"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MohannadNaj/run-subdir/issues"
  },
  "homepage": "https://github.com/MohannadNaj/run-subdir#readme",
  "scripts": {
    "test": "nyc ava --verbose --serial",
    "test:watch": "nyc ava --verbose --serial --watch",
    "ava:watch": "ava --verbose --serial --watch",
    "lint": "standard --fix"
  },
  "files": [
    "src",
    "bin"
  ],
  "ava": {
    "files": [
      "tests/*.spec.js"
    ],
    "sources": [
      "src/**/*",
      "bin/**/*"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "**/tests/**/*"
    ],
    "sourceMap": true,
    "instrument": true,
    "require": [],
    "all": true,
    "reporter": [
      "json",
      "lcov",
      "html",
      "text"
    ]
  },
  "dependencies": {
    "async": "^2.6.2"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "mock-fs": "^4.10.1",
    "nyc": "^14.1.1",
    "standard": "^12.0.1"
  },
  "contributors": [
    "Tatsuya Oiwa <tatsuyaoiw@gmail.com>",
    "Mohannad Najjar <mohannad.najjar@mail.com>"
  ],
  "engines": {
    "node": ">=10.0.0"
  }
}
